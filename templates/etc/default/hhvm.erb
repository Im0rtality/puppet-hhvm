MODE=daemon
USER=<%= scope.lookupvar('hhvm::params::hhvm_user') %>
SOCK=<%= scope.lookupvar('hhvm::service::socket') %>
PORT=<%= scope.lookupvar('hhvm::service::port') %>
PIDFILE=<%= scope.lookupvar('hhvm::service::pid') %>
JITREPO=<%= scope.lookupvar('hhvm::service::jit_repo') %>
CONFIG_FILE="<%= scope.lookupvar('hhvm::service::server_ini') %>"
SYSTEM_CONFIG_FILE="<%= scope.lookupvar('hhvm::service::php_ini') %>"
HDF_CONFIG_FILE="<%= scope.lookupvar('hhvm::service::config_hdf') %>"
ARGS="-c <%= scope.lookupvar('hhvm::service::php_ini') %> -c <%= scope.lookupvar('hhvm::service::server_ini') %> -c <%= scope.lookupvar('hhvm::service::config_hdf') %> -u <%= scope.lookupvar('hhvm::params::hhvm_user') %>"
